#include <stdio.h>

int main()
 {
     // To accept the marks from the user
     printf ("Enter the marks of the student from 0 to 100\n");
     int marks=0;
     scanf("%d",&marks);
     // To check the condition to allot grades on the marks obtained
     if(marks>=0 && marks <=100)
        {
            if(marks >=85 && marks<=100)
             printf ("GRADE A");             //printing grades
            else if(marks >=70 && marks<=84)
              printf ("GRADE B");
            else if(marks >=55 && marks<=69)
              printf ("GRADE C"); 
            else if(marks >=40 && marks<=54)
              printf ("GRADE D");
            else
             printf ("GRADE F");
           }
      else
      printf ("Enter correct marks \n");
    return 0;
}
